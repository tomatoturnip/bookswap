<h1>My Lender List</h1>

<table>
  <thead>
      <th>Title</th>
      <th>Author</th>
      <th>Delete Book</th>
  </thead>

  <tbody>
      <% @lend_books.each do |lend_book| %>
    <tr>
      <td><%= lend_book.book.title %></td>
      <td><%= lend_book.book.author %></td>
      <td><%= link_to "Delete", lend_book_path(lend_book), method: :delete %></td>
    </tr>
      <% end %>
  </tbody>
</table>
<%= button_to "add a book", new_lend_book_path, method: :get %>
